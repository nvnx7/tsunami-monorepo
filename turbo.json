{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "web#build": {
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_RPC_URL_GOERLI",
        "NEXT_PUBLIC_RPC_POLYGON_MUMBAI",
        "NEXT_PUBLIC_RPC_URL_CHIADO"
      ]
    },
    "protocol#build": {
      "env": ["RPC_GOERLI", "PRIVATE_KEYS_TEST", "RPC_CHIADO", "RPC_POLYGON_MUMBAI", "HARDHAT_FORK"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
